<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- normal -->
    <item android:state_focused="false"
        android:state_pressed="false">
        <layer-list>
            <item android:width="78dp"
                android:height="78dp"
                android:gravity="left"
                android:left="4dp">
                <bitmap android:gravity="left|top"
                    android:src="@drawable/bg_center_m_5_left_on" />
            </item>
            <item android:top="17.7dp"
                android:left="29.3dp">
                <bitmap android:gravity="top|left"
                    android:src="@drawable/menu_return_off" />
            </item>
            <item android:left="67.3dp">
                <bitmap android:gravity="right|top"
                    android:src="@drawable/bg_center_m_5_right" />
            </item>
        </layer-list>
    </item>

</selector>