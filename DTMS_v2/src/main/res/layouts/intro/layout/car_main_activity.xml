<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<androidx.drawerlayout.widget.DrawerLayout
		android:id="@+id/dLayout"
		android:layout_width="0dp"
		android:layout_height="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.0">

		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/layout_full"
			android:background="@drawable/bg_main_car"
			android:layout_width="match_parent"
			android:layout_height="match_parent">

			<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/layout_nav"
				android:layout_width="0dp"
				android:layout_height="0dp"
				app:layout_constraintVertical_weight="3.2"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintBottom_toTopOf="@+id/layout_cont">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/layout_nav_top"
					app:layout_constraintVertical_weight="1"
					android:layout_width="0dp"
					android:layout_height="0dp"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toTopOf="@+id/layout_nav_bottom">

					<TextView
						android:id="@+id/txt_nav_logo"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="DAEYANG"
						android:textSize="24dp"
						android:textColor="#FFFFFF"
						android:textStyle="bold"
						android:fontFamily="@font/my_nanum_square_eb"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"/>

					<ImageButton
						android:id="@+id/btn_menu"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingStart="30dp"
						android:paddingTop="30dp"
						android:paddingBottom="30dp"
						android:paddingEnd="20dp"
						android:background="@null"
						android:src="@drawable/selector_button_main_menu"
						android:onClick="onClick"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"/>
				</androidx.constraintlayout.widget.ConstraintLayout>

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/layout_nav_bottom"
					app:layout_constraintVertical_weight="2.5"
					android:layout_width="0dp"
					android:layout_height="0dp"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@+id/layout_nav_top"
					app:layout_constraintBottom_toBottomOf="parent">

					<ImageView
						android:id="@+id/img_main_profile"
						android:layout_width="90dp"
						android:layout_height="90dp"
						android:background="@drawable/img_main_profile"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toTopOf="@+id/txt_car_no"/>

					<TextView
						android:id="@+id/txt_car_no"
						android:layout_marginTop="10dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="00 가 0000"
						android:textSize="22dp"
						android:textColor="#FFFFFF"
						android:textStyle="bold"
						android:fontFamily="@font/noto_sans_kr_bold_hestia"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toBottomOf="@+id/img_main_profile"/>

					<View
						android:id="@+id/view_bar"
						android:layout_marginStart="80dp"
						android:layout_marginEnd="80dp"
						android:layout_width="0dp"
						android:layout_height="45dp"
						android:background="@drawable/bg_hello_bar"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toBottomOf="@+id/txt_car_no"
						app:layout_constraintBottom_toBottomOf="parent">

					</View>

					<TextView
						android:id="@+id/txt_user_hello"
						android:layout_marginTop="5dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						android:text="000님, 안녕하세요."
						android:textSize="20dp"
						android:textColor="#FFFFFF"
						android:textAlignment="center"
						app:layout_constraintStart_toStartOf="@+id/view_bar"
						app:layout_constraintEnd_toEndOf="@+id/view_bar"
						app:layout_constraintTop_toTopOf="@+id/view_bar"
						app:layout_constraintBottom_toBottomOf="@+id/view_bar"/>

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.constraintlayout.widget.ConstraintLayout>

			<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/layout_cont"
				android:layout_width="0dp"
				android:layout_height="0dp"
				app:layout_constraintVertical_weight="5"
				android:background="@android:color/transparent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/layout_nav"
				app:layout_constraintBottom_toTopOf="@+id/layout_call">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/layout_top"
					android:layout_width="0dp"
					android:layout_height="0dp"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toTopOf="@+id/layout_mid">

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/layout_top_left"
						android:layout_marginStart="20dp"
						android:layout_marginEnd="10dp"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="10dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toStartOf="@+id/layout_top_right"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent">

						<Button
							android:id="@+id/btnMenu1"
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:background="@drawable/selector_button_menu1"
							android:onClick="onClick"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:layout_constraintBottom_toBottomOf="parent"/>

					</androidx.constraintlayout.widget.ConstraintLayout>

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/layout_top_right"
						android:layout_marginStart="10dp"
						android:layout_marginEnd="20dp"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="10dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						app:layout_constraintStart_toEndOf="@+id/layout_top_left"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent">

						<Button
							android:id="@+id/btnMenu2"
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:background="@drawable/selector_button_menu2"
							android:onClick="onClick"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:layout_constraintBottom_toBottomOf="parent"/>

					</androidx.constraintlayout.widget.ConstraintLayout>

				</androidx.constraintlayout.widget.ConstraintLayout>

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/layout_mid"
					android:layout_width="0dp"
					android:layout_height="0dp"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@+id/layout_top"
					app:layout_constraintBottom_toTopOf="@+id/layout_bot">

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/layout_mid_left"
						android:layout_marginStart="20dp"
						android:layout_marginEnd="10dp"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="10dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toStartOf="@+id/layout_mid_right"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent">

						<Button
							android:id="@+id/btnMenu3"
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:background="@drawable/selector_button_menu3"
							android:onClick="onClick"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:layout_constraintBottom_toBottomOf="parent"/>

					</androidx.constraintlayout.widget.ConstraintLayout>

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/layout_mid_right"
						android:layout_marginStart="10dp"
						android:layout_marginEnd="20dp"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="10dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						app:layout_constraintStart_toEndOf="@+id/layout_mid_left"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent">

						<Button
							android:id="@+id/btnMenu4"
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:background="@drawable/selector_button_menu4"
							android:onClick="onClick"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:layout_constraintBottom_toBottomOf="parent"/>

					</androidx.constraintlayout.widget.ConstraintLayout>

				</androidx.constraintlayout.widget.ConstraintLayout>

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/layout_bot"
					android:layout_width="0dp"
					android:layout_height="0dp"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@+id/layout_mid"
					app:layout_constraintBottom_toBottomOf="parent">

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/layout_bot_left"
						android:layout_marginStart="20dp"
						android:layout_marginEnd="10dp"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="10dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintEnd_toStartOf="@+id/layout6"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent">

						<Button
							android:id="@+id/btnMenu5"
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:background="@drawable/selector_button_menu5"
							android:onClick="onClick"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:layout_constraintBottom_toBottomOf="parent"/>

					</androidx.constraintlayout.widget.ConstraintLayout>

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/layout6"
						android:layout_marginStart="10dp"
						android:layout_marginEnd="20dp"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="10dp"
						android:layout_width="0dp"
						android:layout_height="0dp"
						app:layout_constraintStart_toEndOf="@+id/layout_bot_left"
						app:layout_constraintEnd_toEndOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent">

						<Button
							android:id="@+id/btnMenu6"
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:background="@drawable/selector_button_menu6"
							android:onClick="onClick"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:layout_constraintBottom_toBottomOf="parent"/>

					</androidx.constraintlayout.widget.ConstraintLayout>

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.constraintlayout.widget.ConstraintLayout>

			<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/layout_call"
				app:layout_constraintVertical_weight="0.7"
				android:layout_marginStart="40dp"
				android:layout_marginEnd="40dp"
				android:layout_marginBottom="15dp"
				android:layout_width="0dp"
				android:layout_height="0dp"
				android:background="@drawable/border_main_call"
				android:onClick="onClick"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/layout_cont"
				app:layout_constraintBottom_toBottomOf="parent">

				<TextView
					android:id="@+id/txt_center"
					android:layout_marginStart="70dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="고객센터"
					android:textColor="#ffffff"
					android:textSize="21dp"
					android:textStyle="bold"
					android:textAlignment="textEnd"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toStartOf="@+id/txt_call"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toBottomOf="parent"/>

				<TextView
					android:id="@+id/txt_call"
					android:layout_marginStart="15dp"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:text="0000-0000"
					android:textColor="#ffffff"
					android:textSize="23dp"
					android:textStyle="bold"
					app:layout_constraintStart_toEndOf="@+id/txt_center"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toBottomOf="parent" />

			</androidx.constraintlayout.widget.ConstraintLayout>

		</androidx.constraintlayout.widget.ConstraintLayout>

		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/menu_layout"
			android:layout_width="280dp"
			android:layout_height="match_parent"
			android:layout_gravity="right">

			<include
				layout="@layout/menu_open"
				android:layout_width="0dp"
				android:layout_height="0dp"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent" />

		</androidx.constraintlayout.widget.ConstraintLayout>

	</androidx.drawerlayout.widget.DrawerLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
